# Solana RPC URL
RPC_URL=https://api.mainnet-beta.solana.com

# Path to deploy authority keypair JSON file (required)
DEPLOY_AUTHORITY_KEYPAIR=/path/to/keypair.json

# SQLite database path for transaction tracking
DATABASE_PATH=crank.db

# Priority fee in microlamports per compute unit
PRIORITY_FEE=100000

# Jito tip in lamports (0.0002 SOL = 200000 lamports)
JITO_TIP=200000

# Enable Jito bundle sending (true/false)
USE_JITO=true

# Helius API key for fast transaction sending (optional)
# HELIUS_API_KEY=your-api-key-here

# Poll interval in milliseconds
POLL_INTERVAL_MS=400

LUT_ADDRESS=pubkey
