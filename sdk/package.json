{
  "name": "evore-sdk",
  "version": "2.2.0",
  "description": "JavaScript SDK for the Evore automining program - the cheapest and most secure managed miner solution on Solana",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "default": "./dist/constants.js"
    },
    "./pda": {
      "types": "./dist/pda.d.ts",
      "default": "./dist/pda.js"
    },
    "./accounts": {
      "types": "./dist/accounts.d.ts",
      "default": "./dist/accounts.js"
    },
    "./instructions": {
      "types": "./dist/instructions.d.ts",
      "default": "./dist/instructions.js"
    },
    "./transactions": {
      "types": "./dist/transactions.d.ts",
      "default": "./dist/transactions.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir -p dist && cp src/*.js src/*.d.ts dist/",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "solana",
    "ore",
    "mining",
    "automining",
    "evore",
    "sdk",
    "web3",
    "blockchain",
    "crypto"
  ],
  "author": "kriptikz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kriptikz/evore.git"
  },
  "homepage": "https://github.com/kriptikz/evore/tree/master/sdk#readme",
  "bugs": {
    "url": "https://github.com/kriptikz/evore/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.98.0"
  },
  "devDependencies": {
    "@solana/web3.js": "^1.98.0"
  }
}
